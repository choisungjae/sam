package control_p;

public class Zam2 {

	public static void main(String[] args) {
		String cu="커피이름";
		int price;
	    switch(cu) {
		case "아메리카노":
			price =2000;
			break;
		case "아시아노":
			price=3000;
			break;
		case "아프리카노":
			price= 4000;
			break;
			
		}
	       System.out.println("cu");
		
		
		
		
		
		//아메2000
		//아시아노 3000
		//아프리카노4000
		//10000이상이면 배달가능 / 8000이상이면 포인트추가,미만이면 기본

	}

}
