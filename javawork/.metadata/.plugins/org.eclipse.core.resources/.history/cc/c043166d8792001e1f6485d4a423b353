package Restaurant;
import java.util.Comparator;
import java.util.TreeSet;

//1번문제 반,등수,이름 출력
// 만약다하면 2번문제 타입,반,성별,등수,이름순으로 출력
//3번 입력값에 따라 타입,반,성별 선택하고 등수,이름순으로 출력하세요

//타입 = 일반,예체능
//다하면  2차  집어넣는 값에따라 정렬알아서 바뀌어라
//타입별로비교 - 같으면 반별로해라 - 그다음은 등수나 뭐 그런거로 정렬해라

enum type{
	일반, 예체능
}
class Stud{
	type sc;
	String name;
	int jum;
	
	public Stud(String sc, String name, int jum) {
		this.sc = type.valueOf(sc);
		this.name = name;
		this.jum = jum;
	}

	@Override
	public String toString() {
		return sc + "\t" + name + "\t" + jum;
	}
}

class Studrank implements Comparator{

	public int compare(Object o1, Object o2) {
	    Stud me=(Stud)o1; 
	    Stud you=(Stud)o2; 
		
		return 0;
		
	}
}

 

public class Tr1204 {

	public static void main(String[] args) {
		TreeSet st = new TreeSet(new Studrank());

		st.add(new Stud("일반","최성재",72));
		st.add(new Stud("예체능","최성재4",72));
		st.add(new Stud("일반","최성재3",72));
		st.add(new Stud("예체능","최성재2",72));
	
	
	for (Object ob: st) {
		System.out.println(ob);
	}
	} 

}
