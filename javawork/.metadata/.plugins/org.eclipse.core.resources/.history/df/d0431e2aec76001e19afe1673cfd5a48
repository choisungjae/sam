package control_p;

public class SwitchSampleMain {

	public static void main(String[] args) {
		
		double avg=100.54;
		System.out.println((int)avg/10);
		 
		switch((int)avg/10){
		case 9,10:
			System.out.println("수");
		 break;
		case 8:
			System.out.println("우");
		 break;
		case 7:
			System.out.println("미");
		 break;
		case 6:
			System.out.println("양");
		 break;
		case 5:
			System.out.println("가");
		 break;
		}

	}

}
